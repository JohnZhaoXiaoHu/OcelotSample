{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  /*服务治理第三方组件Consul相关配置参数*/
  "ServiceDiscovery": {
    "ServiceName": "ApiOne", //本WebApi项目的服务名称，可以随便取名，将显示Consul的管理页面上，届时通过该服务名查找可用的服务站点
    "Consul": {
      "HttpEndpoint": "http://127.0.0.1:8500",
      "TcpEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  }
}
